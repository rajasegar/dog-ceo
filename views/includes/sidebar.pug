h5.h5 Breeds
nav#sidebarMenu.col-md-3.col-lg-2.d-md-block.bg-light.sidebar.collapse
  .position-sticky
    ul.nav.flex-column
      each breed in breeds
        li.nav-item
          if breed.subBreeds
            a.nav-link(href='/breeds/sub/' + breed.id)
              | #{breed.name}         
          else
            a.nav-link(href='/breeds/' + breed.id)
              | #{breed.name}

